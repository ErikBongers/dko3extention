{
  "manifest_version": 3,
  "name": "DKO3 Helper (beta)",
  "description": "Stedelijk Onderwijs Antwerpen. Helper extension voor DKO3.",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/dko3.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js", "scripts/scrape.js", "scripts/build.js", "scripts/convert.js"],
      "css": ["css/trimesterTable.css"],
      "matches": [
        "https://administratie.dko3.cloud/*"
      ]
    }
  ]
}