{
  "manifest_version": 3,
  "name": "DKO3 Helper (beta)",
  "description": "Stedelijk Onderwijs Antwerpen. Helper extension voor DKO3.",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/dko3.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/lessen/convert.js",
        "scripts/lessen/setup.js",
        "scripts/content.js",
        "scripts/lessen/scrape.js",
        "scripts/lessen/build.js"
      ],
      "css": [
        "css/general.css",
        "css/trimesterTable.css"
      ],
      "matches": [
        "https://administratie.dko3.cloud/*"
      ]
    }
  ]
}